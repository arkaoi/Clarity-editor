ENDPOINT="http://localhost:8080/database"
TOTAL_KEYS=1000
DELAY_SECONDS=0.001

for i in $(seq 1 $TOTAL_KEYS); do
    curl -X PUT -s -H "Content-Type: application/json" \
        -d "{\"value\": \"value$i\"}" \
        "$ENDPOINT/key$i" 

    echo "Ключ $i отправлен"
    
    sleep $DELAY_SECONDS
done

echo -e "\nВсе $TOTAL_KEYS ключей успешно отправлены!"
